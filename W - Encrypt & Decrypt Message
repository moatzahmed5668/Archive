#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include<bits/stdc++.h>
#include <iomanip>
#include <iostream>
#include<bitset>
#include <string>
#include <string.h>
#include<algorithm>
#include <cstdio>
using namespace std;


int main() {

string original="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789";
string key="PgEfTYaWGHjDAmxQqFLRpCJBownyUKZXkbvzIdshurMilNSVOtec#@_!=.+-*/";
int q;
cin>>q;
string input;
cin>>input;
if(q==1)
{
    for(int i  = 0 ;i<input.size();i++)
    {
        int index=original.find(input[i]);
        cout<<key[index];
    }
}
else{
    for(int i=0 ; i<input.size();i++)
    {
        int index=key.find(input[i]);
        cout<<original[index];
    }
}

    return 0;
}
