#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include<bits/stdc++.h>
#include <iomanip>
#include <iostream>
#include<bitset>
#include <string>
#include <string.h>
#include<algorithm>
#include <cstdio>
using namespace std;


int main() {
string s;
getline(cin,s);
while(s.find("EGYPT")!=std::string::npos){
    int index=s.find("EGYPT");
    s.replace(index,5," ");
}
cout<<s<<endl;

    return 0;
}
